apiVersion: batch/v1
kind: Job
metadata:
  name: testexecutor-executorjob-0
  labels:
    app: kubeexecutor
spec:
  completions: 4
  parallelism: 4
  template:
    spec:
      containers:
      - name: kubeexecutor-jobcontainer-0
        image: busybox
        command: ["sh", "-c", "echo helloworld!"]
      - name: kubeexecutor-jobcontainer-1
        image: busybox
        command: ["sh", "-c", "echo helloworld!"]
      restartPolicy: Never
  backoffLimit: 4


{
    "conditions": {
        "executortype": "lumi-hpcexecutor",
        "nodes": 1,
        "processes_per_node": 1,
        "mem": "5G",
        "gpu": {
            "count": 1
        },
        "walltime": 60
    },
    "funcname": "execute",
    "kwargs": {
        "cmd": "python3",
        "args": [
            "/cfs/src/fetch_webpage.py"
        ],
        "docker-image": "johan/nettest",
        "rebuild-image": false,
        "keep_snapshots": false
    },
    "fs": {
        "mount": "/cfs",
        "dirs": [
            {
                "label": "/src",
                "dir": "/src",
                "keepfiles": true,
                "onconflicts": {
                    "onstart": {
                        "keeplocal": false
                    },
                    "onclose": {
                        "keeplocal": true
                    }
                }
            }
        ]
    },
    "maxexectime": 100,
    "maxretries": 3
}

{
    "conditions": {
        "executortype": "gpu-mlexecutor"
    },
    "funcname": "execute",
    "kwargs": {
        "cmd": "python3",
        "args": [
            "/tmp/helloworld/helloworld.py"
        ],
        "keep_snapshots": false
    },
    "fs": [
        {
            "label": "src",
            "dir": "/tmp/helloworld"
        },
        {
            "label": "data",
            "dir": "/tmp/data"
        }
    ],
    "maxwaittime": -1,
    "maxexectime": 10,
    "maxretries": 3
}

{
    "conditions": {
        "executortype": "k8s-executor"
    },
    "funcname": "execute",
    "kwargs": {
        "cmd": "python3",
        "args": [
            "/tmp/classifier/classifier_download.py"
        ],
        "keep_snapshots": false
    },
    "fs": [
        {
            "label": "src",
            "dir": "/tmp/classifier"
        },
        {
            "label": "results",
            "dir": "/tmp/helloworld/result",
            "sync_on_completion": true
        }
    ],
    "maxwaittime": -1,
    "maxexectime": 100,
    "maxretries": 3
}

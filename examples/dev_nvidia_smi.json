{
    "conditions": {
        "executortype": "dev-hpcexecutor",
        "nodes": 1,
        "processespernode": 1,
        "cpu": "1000m",
        "mem": "100Mi",
        "walltime": 60,
        "gpu": {
            "count": 1
        }
    },
    "funcname": "execute",
    "kwargs": {
        "cmd": "nvidia-smi",
        "docker-image": "tensorflow/tensorflow:2.14.0rc1-gpu"
    },
    "maxwaittime": -1,
    "maxexectime": 10,
    "maxretries": 3
}

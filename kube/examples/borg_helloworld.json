{
    "conditions": {
        "executortype": "borg-kubeexecutor",
        "nodes": 1,
        "processes-per-node": 1,
        "mem": "1Gi",
        "cpu": "500m",
        "walltime": 60,
        "gpu": {
            "count": 0
        }
    },
    "funcname": "execute",
    "kwargs": {
        "cmd": "python3",
        "docker-image": "python:3.12-rc-bookworm",
        "rebuild-image": false,
        "args": [
            "/cfs/src/{processid}/helloworld.py",
            "2"
        ],
        "keep_snapshots": false
    },
    "fs": {
        "mount": "/cfs",
        "snapshots": [
            {
                "snapshotid": "{snapshotid}",
                "label": "/src",
                "dir": "/src/{processid}",
                "keepfiles": false,
                "keepsnaphot": false
            }
        ]
    },
    "maxwaittime": -1,
    "maxexectime": 100,
    "maxretries": 3
}
